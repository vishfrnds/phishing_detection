{
  "manifest_version": 2,
  "name": "One-click Kittens",
  "description": "This extension demonstrates a 'browser action' with kittens.",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.html"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://localhost:8084/Anti%20Phishing",
    "tabs"
  ]
}
