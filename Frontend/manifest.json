{
  "manifest_version": 2,
  "name": "Anti-Phishing",
  "description": "This extension protects you from phishing websites.",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.html"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://localhost:8080/anti_phishing_response/",
    "tabs"
  ]
}
